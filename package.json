{
  "name": "automation-dashboard",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20 || 21 || 22"
  },
  "main": "./src/frontend/index.tsx",
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "build": "tsc --noEmit && vite build",
    "start": "sirv dist --cors --single --host",
    "start:dev": "vite",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./src/frontend",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/frontend/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "vite build",
    "bundle-profile:analyze": "vite build && open dist/bundle-analysis.html",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "^29.5.14",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-react": "^4.6.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv-webpack": "^7.1.1",
    "eslint": "^9.17.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "imagemin": "^9.0.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-fixed-jsdom": "^0.0.9",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^7.6.3",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass-loader": "^16.0.4",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "url-loader": "^4.1.1",
    "vite": "^7.0.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@patternfly/react-charts": "^8.2.2",
    "@patternfly/react-component-groups": "^6.2.1",
    "@patternfly/react-core": "^6.2.2",
    "@patternfly/react-icons": "^6.2.2",
    "@patternfly/react-styles": "^6.2.2",
    "@reduxjs/toolkit": "^2.5.0",
    "@types/react-dom": "^19.0.3",
    "axios": "^1.7.9",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "redux-thunk": "^3.1.0",
    "sass": "^1.83.4",
    "sirv-cli": "^3.0.0",
    "victory-area": "^37.3.5",
    "victory-bar": "^37.3.5",
    "victory-box-plot": "^37.3.5",
    "victory-chart": "^37.3.5",
    "victory-core": "^37.3.5",
    "victory-create-container": "^37.3.5",
    "victory-group": "^37.3.5",
    "victory-legend": "^37.3.5",
    "victory-line": "^37.3.5",
    "victory-pie": "^37.3.5",
    "victory-scatter": "^37.3.5",
    "victory-stack": "^37.3.5",
    "victory-voronoi-container": "^37.3.5",
    "zustand": "^5.0.6"
  },
  "resolutions": {
    "@patternfly/react-core": "6.2.2"
  }
}
