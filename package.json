{
  "name": "automation-dashboard",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.19 <21 || >=22.12"
  },
  "main": "./src/frontend/index.tsx",
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "build": "tsc --noEmit && vite build",
    "start": "sirv dist --cors --single --host",
    "start:dev": "vite",
    "test": "playwright test",
    "eslint": "eslint --ext .tsx,.js ./src/frontend",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/frontend/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "vite build",
    "bundle-profile:analyze": "vite build && open dist/bundle-analysis.html",
    "clean": "rimraf dist",
    "i18n": "i18next-parser --config i18next-parser.config.cjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "i18next-parser": "^9.0.2",
    "imagemin": "^9.0.1",
    "jest-environment-jsdom": "30.2.0",
    "jest-fixed-jsdom": "^0.0.11",
    "mini-css-extract-plugin": "^2.9.4",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^7.9.6",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.1.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass-loader": "^16.0.6",
    "svg-url-loader": "^8.0.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "url-loader": "^4.1.1",
    "vite": "^7.2.2",
    "vite-plugin-static-copy": "^3.1.5"
  },
  "dependencies": {
    "@patternfly/react-charts": "^8.4.0",
    "@patternfly/react-component-groups": "^6.3.0",
    "@patternfly/react-core": "^6.4.0",
    "@patternfly/react-icons": "^6.4.0",
    "@patternfly/react-styles": "^6.4.0",
    "@types/react-dom": "^19.2.3",
    "axios": "^1.13.2",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.3",
    "sass": "^1.94.1",
    "sirv-cli": "^3.0.1",
    "victory-area": "^37.3.6",
    "victory-bar": "^37.3.6",
    "victory-box-plot": "^37.3.6",
    "victory-chart": "^37.3.6",
    "victory-core": "^37.3.6",
    "victory-create-container": "^37.3.6",
    "victory-group": "^37.3.6",
    "victory-legend": "^37.3.6",
    "victory-line": "^37.3.6",
    "victory-pie": "^37.3.6",
    "victory-scatter": "^37.3.6",
    "victory-stack": "^37.3.6",
    "victory-voronoi-container": "^37.3.6",
    "zustand": "^5.0.8"
  },
  "resolutions": {
    "@patternfly/react-core": "6.4.0"
  },
  "overrides": {
    "chokidar": "^4.0.3",
    "micromatch": "^4.0.8"
  }
}
